    "#import random(生成随机数字)#\n",
    "import random\n",
    "answer=random.randint(1,90)\n",
    "n=int(input(\"please input num(1-90):\"))\n",
    "while n!=answer:\n",
    "    if n>=answer:\n",
    "        n = int(input(\"数字大了，请重新输入:\"))\n",
    "    elif n<answer:\n",
    "        n = int(input(\"数字小了，请重新输入：\"))\n",
    "print(\"恭喜！输入正确！\")"
   ]
